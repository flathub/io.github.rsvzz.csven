{
  "id": "io.github.rsvzz.csven",
  "runtime": "org.gnome.Platform",
  "runtime-version": "49",
  "sdk": "org.gnome.Sdk",
  "command": "csven",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin",
    "env": {
      "HOME_CARGO": "/run/build/csven/cargo"
    }
  },
  "finish-args": [
    "--share=ipc",
    "--device=dri",
    "--socket=fallback-x11",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include"
  ],
  "modules": [
    {
      "name": "csven",
      "buildsystem": "meson",
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/rsvzz/csven-rs/archive/refs/tags/v0.1.23.tar.gz",
          "sha256": "bde1075cb17bd594a73dc11a1df061279be714577000f491de24ef041c1c2e0f"
        }
      ]
    }
  ]
}