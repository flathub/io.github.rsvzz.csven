{
  "id": "io.github.rsvzz.csven",
  "runtime": "org.gnome.Platform",
  "runtime-version": "49",
  "sdk": "org.gnome.Sdk",
  "command": "csven",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin",
    "env": {
      "HOME_CARGO": "/run/build/csven/cargo"
    }
  },
  "finish-args": [
    "--share=ipc",
    "--device=dri",
    "--socket=fallback-x11",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include"
  ],
  "modules": [
    {
      "name": "csven",
      "buildsystem": "meson",
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/rsvzz/csven-rs/releases/download/v0.1.28/csven-rs_v0.1.28_all.tar.xz",
          "sha256": "58c9a33067c2d3577844e4f4badb15ddf6f7af96b8751de6fd3f40a19dea95ae"
        }
      ]
    }
  ]
}