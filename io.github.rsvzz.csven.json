{
  "id": "io.github.rsvzz.csven",
  "runtime": "org.gnome.Platform",
  "runtime-version": "49",
  "sdk": "org.gnome.Sdk",
  "command": "csven",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin"
  },
  "finish-args": [
    "--share=ipc",
    "--device=dri",
    "--socket=fallback-x11",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include"
  ],
  "modules": [
    {
      "name": "csven",
      "buildsystem": "meson",
      "sources": [
         {
          "type":"cargo",
          "path": "cargo-sources.json"
        },
        {
          "type": "archive",
          "url": "https://github.com/rsvzz/csven-rs/archive/refs/tags/v0.1.21.tar.gz",
          "sha256": "362e486635587b102821e401d06713161538a60d3e51d66b8cfc5c9de3264ccc"
        }
      ]
    }
  ]
}