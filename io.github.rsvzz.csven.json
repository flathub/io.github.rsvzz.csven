{
  "id": "io.github.rsvzz.csven",
  "runtime": "org.gnome.Platform",
  "runtime-version": "49",
  "sdk": "org.gnome.Sdk",
  "command": "csven",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin"
  },
  "finish-args": [
    "--share=ipc",
    "--device=dri",
    "--socket=fallback-x11",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include"
  ],
  "modules": [
    {
      "name": "csven",
      "buildsystem": "meson",
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/rsvzz/csven-rs/archive/refs/tags/v0.1.22.tar.gz",
          "sha256": "c98d6a998c3bc747b44de64cf61b5ff8c6d2568aaa123a7a13e5bcc2e1f4138f"
        }
      ]
    }
  ]
}